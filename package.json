{
  "name": "XAudioJS",
  "version": "0.0.1",
  "description": "XAudioJS播放器",
  "main": "dist/XAudioJS.min.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "webpack-dev-server --progress --config ./build/webpack.config.js",
    "test": "rollup -w -c ./build/rollup.config.js",
    "build": "rollup -c ./build/rollup.config.js",
    "lint": "npm run eslint && npm run prettier",
    "eslint": "eslint --ext .js,.ts .",
    "eslint:fix": "eslint --ext .js,.ts --fix .",
    "prettier": "prettier -l \"./**/*.{js,json}\"",
    "prettier:fix": "prettier --write -l \"./**/*.{js,json}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/halldwang/XAudioJS.git"
  },
  "keywords": [
    "xAudio",
    "x-audio",
    "XAudioJS"
  ],
  "author": "halldwang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/halldwang/XAudioJS/issues"
  },
  "homepage": "https://github.com/halldwang/XAudioJS/",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run lint"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "core-js": "^3.1.3",
    "husky": "^3.0.8",
    "prettier": "1.18.2",
    "html-webpack-plugin": "^3.2.0",
    "rollup": "^1.14.6",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.0.1",
    "rollup-plugin-terser": "^5.0.0",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.7.1",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.4.0",
    "eslint-config-alloy": "^3.0.0"
  }
}
